{
    "deploy": {
        "steps": [
            {
                "apply": {
                    "file": "data/cass-operator/init.yaml"
                }
            },
            {
                "wait": {
                    "duration": 10
                }
            },
            {
                "apply": {
                    "file": "data/cass-operator/bundle.yaml",
                    "operator": true
                }
            }
        ]
    },
    "crd_name": "cassandradatacenters.cassandra.datastax.com",
    "seed_custom_resource": "data/cass-operator/cr.yaml"
}