        "\\[\\'secret\\'\\]\\[\\'webhook\\-server\\-cert\\'\\]\\[\\'data\\'\\]\\[\\'ca\\.crt\\'\\]",
        "\\[\\'secret\\'\\]\\[\\'webhook\\-server\\-cert\\'\\]\\[\\'data\\'\\]\\[\\'tls\\.crt\\'\\]",
        "\\[\\'secret\\'\\]\\[\\'webhook\\-server\\-cert\\'\\]\\[\\'data\\'\\]\\[\\'tls\\.key\\'\\]",
